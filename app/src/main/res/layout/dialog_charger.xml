    <ProgressBar
        android:id="@+id/progressBarDialoigChargerCotizalomx"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone" />

</RelativeLayout>